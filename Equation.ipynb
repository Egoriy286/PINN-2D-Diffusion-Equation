   "source": [
    "import sympy as sp\n",
    "C = 0.1        # Коэффициент C\n",
    "a = 1          # Коэффициент a\n",
    "x, t = sp.symbols(\"x t\")\n",
    "u = C * sp.exp(x + a**2 * t) + sp.sin(x)\n",
    "\n",
    "func = sp.sin(x)\n",
    "\n",
    "pde = sp.diff(u, t) - a**2 * sp.diff(u, x, x) - func\n",
    "\n",
    "pde"
   ]
